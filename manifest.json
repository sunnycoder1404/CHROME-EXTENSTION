{
    "manifest_version": 3,
    "name": "Ultimate AI Chrome Extension",
    "version": "1.0",
    "description": "Revolutionize your browsing experience with AI automation, personalized themes, user points system, and seamless configuration management.",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      },
      "default_title": "Ultimate AI Extension"
    },
    "permissions": [
      "storage",
      "notifications",
      "activeTab",
      "scripting",
      "contextMenus",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "options_page": "options.html",
    "web_accessible_resources": [
      {
        "resources": ["icon16.png", "icon48.png", "icon128.png", "styles.css"],
        "matches": ["<all_urls>"]
      }
    ],
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "default_locale": "en",
    "commands": {
      "toggle_ai": {
        "suggested_key": {
          "default": "Ctrl+Shift+A"
        },
        "description": "Toggle AI Automation"
      },
      "open_popup": {
        "suggested_key": {
          "default": "Ctrl+Shift+U"
        },
        "description": "Open the Extension Popup"
      }
    },
    "update_url": "https://your-update-url.com/update.xml",
    "homepage_url": "https://your-website.com",
    "minimum_chrome_version": "90",
    "offline_enabled": true,
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "description": "A powerful Chrome extension that utilizes AI to enhance user experience with automation, configuration management, and customizable themes.",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "web_accessible_resources": [{
      "resources": ["icon16.png", "icon48.png", "icon128.png"],
      "matches": ["<all_urls>"]
    }],
    "background": {
      "service_worker": "background.js"
    }
  }
  
